	<queryString>
		<![CDATA[SELECT
     penilaian.`id` AS penilaian_id,
     penilaian.`tanggal` AS penilaian_tanggal,
     sub_penilaian.`id` AS sub_penilaian_id,
     sub_penilaian.`id_penilaian` AS sub_penilaian_id_penilaian,
     sub_penilaian.`nama` AS sub_penilaian_nama,
     sub_penilaian.`score` AS sub_penilaian_score,
     sub_penilaian.`ranking` AS sub_penilaian_ranking,
     siswa.`id` AS siswa_id,
     siswa.`nama` AS siswa_nama,
     siswa.`jenkel` AS siswa_jenkel,
     siswa.`hp` AS siswa_hp,
     siswa.`semester` AS siswa_semester,
     siswa.`kelas` AS siswa_kelas
FROM
     `penilaian` penilaian INNER JOIN `sub_penilaian` sub_penilaian ON penilaian.`id` = sub_penilaian.`id_penilaian`
     INNER JOIN `siswa` siswa ON sub_penilaian.`nama` = siswa.`nama` WHERE penilaian.id = $P{id}]]>
	</queryString>
